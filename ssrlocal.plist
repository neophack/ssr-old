<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>ssr.local</string>
    <key>KeepAlive</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
        <string>python</string>
        <string>ROOT/local.py</string>
        <string>-c</string>
        <string>ROOT/config-local.json</string>
    </array>
    <key>StandardOutPath</key>
    <string>/tmp/ssrlocal.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/ssrlocal.log</string>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
